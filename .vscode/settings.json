{
    "editor.formatOnType": true,
    "files.autoSave": "afterDelay",
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/built/**": true,
        "**/node_modules/**": true,
        "**/yotta_modules/**": true,
        "**/yotta_targets": true,
        "**/pxt_modules/**": true
    },
    "search.exclude": {
        "**/built": true,
        "**/node_modules": true,
        "**/yotta_modules": true,
        "**/yotta_targets": true,
        "**/pxt_modules": true
    },
    "workbench.colorCustomizations": {
        "activityBar.foreground": "#CECC67",
        "activityBar.background": "#000000",
        "activityBar.inactiveForeground": "#643D00",
        "activityBarBadge.foreground": "#000000",
        "activityBarBadge.background": "#6BD73F",
        "statusBar.foreground": "#B6EDFF",
        "statusBar.background": "#1C718D",
        "statusBarItem.hoverBackground": "#001015",
        "statusBarItem.remoteForeground": "#EDEEFF",
        "statusBarItem.remoteBackground": "#4D50B5",
        "statusBar.debuggingForeground": "#00090C",
        "statusBar.debuggingBackground": "#52A7C4"
    }
}